{
  "name": "blogApp",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "production": "npm run build && node dist/app",
    "build": "babel ./server/ -d ./dist",
    "server": "nodemon server/app --exec babel-node",
    "client": "npm run start:dev --prefix client",
    "start-dev": "concurrently \" npm run server\"  \" npm run client\" ",
    "start": "concurrently \" npm run production\"  \" npm run client\" ",
    "unit:test": "nyc mocha --require @babel/register server/tests/units/index.js --reporter nyan --timeout 15000 --exit ",
    "int:test": "nyc mocha --require @babel/register server/tests/integrations/index.js --reporter nyan --timeout 15000 --exit ",
    "test": "nyc mocha --require @babel/register server/tests/**.test.js --reporter nyan --timeout 15000 --exit "
  },
  "keywords": [],
  "author": "maruf",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "idempotent-babel-polyfill": "^7.4.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.4",
    "sequelize-cli": "^5.5.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "supertest": "^4.0.2"
  }
}
